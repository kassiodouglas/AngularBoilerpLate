{
  "name": "boilerplate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve": "ng serve --configuration development --host 0.0.0.0  --port 3000",

    "g-component": "node src/commands/generate.js --path=components",
    "g-guard": "node src/commands/generate.js --path=guards",
    "g-interceptor": "node src/commands/generate.js --path=interceptors",
    "g-page": "node src/commands/generate.js --path=pages",
    "g-service": "node src/commands/generate.js --path=services",
    "g-feature": "node src/commands/generate.js --path=features",
    "g-module": "node src/commands/generate.js --path=modules",
    "g-pipe": "node src/commands/generate.js --path=pipes",
    "g-directive": "node src/commands/generate.js --path=directives",

    "gc": "node src/commands/generate.js --path=components",
    "gg": "node src/commands/generate.js --path=guards",
    "gi": "node src/commands/generate.js --path=interceptors",
    "gs": "node src/commands/generate.js --path=services",
    "gf": "node src/commands/generate.js --path=features",
    "gm": "node src/commands/generate.js --path=modules"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "moment": "^2.30.1",
    "notiflix": "^3.2.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@types/aos": "^3.0.7",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2"
  }
}
