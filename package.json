{
  "name": "boilerplate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",

    "serve": "ng serve --configuration development --host 0.0.0.0  --port 3000",
    "serve:test": "ng serve --configuration test --host 0.0.0.0  --port 3000",
    "serve:mock": "ng serve --configuration mock --host 0.0.0.0  --port 3000",
    "serve:prod": "ng serve --configuration production --host 0.0.0.0  --port 3000",

    "build": "ng build --configuration development",
    "build:test": "ng build --configuration test",
    "build:mock": "ng build --configuration mock",
    "build:prod": "ng build --configuration production",

    "make:guard":"node src/commands/make.js --dir=guard",
    "make:interceptor":"node src/commands/make.js --dir=interceptor",
    "make:feature":"node src/commands/make.js --dir=feature",
    "make:page":"node src/commands/make.js --dir=page",
    "make:component":"node src/commands/make.js --dir=component",
    "make:directive":"node src/commands/make.js --dir=directive",
    "make:model":"node src/commands/make.js --dir=model",
    "make:pipe":"node src/commands/make.js --dir=pipe",
    "make:service":"node src/commands/make.js --dir=service"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@faker-js/faker": "^9.6.0",
    "@lithiumjs/angular": "^8.0.1",
    "@lithiumjs/ngx-virtual-scroll": "^0.3.3",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "miragejs": "^0.1.48",
    "moment": "^2.30.1",
    "ngx-mask": "^19.0.6",
    "notiflix": "^3.2.8",
    "rxjs": "~7.8.0",
    "signature_pad": "^5.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@types/aos": "^3.0.7",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2"
  }
}
